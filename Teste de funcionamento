source("EstadoAspirador.R")
source("BuscaDesinformadaAspirador.R")

####################################

contexto <- c(1,1,1,1)

estado1Busca <- Aspirador()
estado1Busca$posicao <- 1
estado1Busca$desc <- 4
estado1Busca$contexto <- contexto

buscaEmLargura(estado1Busca)

####################################

